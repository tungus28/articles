<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Главная{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
		<h5>{{name|upper}}</h5>
		<h4>{{article.title}}</h4>
		<p>{{article.content}}</p>
		{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
